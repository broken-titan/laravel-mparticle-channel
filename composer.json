{
    "name": "broken-titan/laravel-mparticle-channel",
    "description": "mParticle notification channel for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marshall Miller",
            "email": "contact@brokentitan.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "mockery/mockery": "1.4.4",
        "phpunit/phpunit": "9.5.10",
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrokenTitan\\MParticle\\Providers\\MParticleServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "BrokenTitan\\MParticle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor\\bin\\phpunit"
        ]
    }
}
